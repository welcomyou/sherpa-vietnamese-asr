# Auto detect text files and perform LF normalization
* text=auto

# Python files
*.py text eol=lf
*.pyw text eol=lf

# Config files
*.ini text eol=lf
*.json text eol=lf
*.txt text eol=lf
*.md text eol=lf
*.yml text eol=lf
*.yaml text eol=lf

# Scripts
*.bat text eol=crlf
*.cmd text eol=crlf
*.ps1 text eol=crlf

# Binary files
*.exe binary
*.dll binary
*.pyd binary
*.so binary
*.dylib binary
*.wav binary
*.mp3 binary
*.onnx binary
*.zip binary
